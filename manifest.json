{
  "manifest_version": 2,
  "name": "Mumbo Jumbo",
  "description": "Language learning gamified. Form sentences in your target language to improve your conversational skills",
  "version": "1.1.0",
  "permissions": [
    "storage"
  ],
  "browser_action": {
    "default_popup": "index.html"
  },
  "icons": { 
    "64": "assets/worldText64.png",
   "128": "assets/worldText128.png" 
  },
  "content_security_policy": "script-src 'self' https://code.responsivevoice.org/responsivevoice.js; object-src 'self'"
}